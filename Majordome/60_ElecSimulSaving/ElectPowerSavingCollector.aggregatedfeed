ElectricitySamples:Publish( ElectPowerSavingStatistics:getSamplesNumber("Saving") )
ElectricitySaving:Publish( ElectPowerSavingStatistics:getSum("Saving") )
ElectricityInjection:Publish( ElectPowerSavingStatistics:getSum("Injection") )

